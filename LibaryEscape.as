package{		//greensock imports	import com.greensock.events.LoaderEvent;	import com.greensock.loading.ImageLoader;	import com.greensock.loading.LoaderMax;	import com.greensock.loading.SWFLoader;	import com.greensock.loading.XMLLoader;	import com.greensock.loading.core.LoaderItem;		import flash.display.MovieClip;	import flash.display.Sprite;	import flash.events.Event;
		public class LibaryEscape extends MovieClip	{		private var loadque:LoaderMax;				public function LibaryEscape()		{			loadque = new LoaderMax({name:"mainque",onProgress:progressHandler, onComplete:completeHandler, onError:errorHandler});						loadque.append(new ImageLoader("img/photo.jpg",{name:"photo"}));			loadque.append(new XMLLoader("xml/test.xml",{name:"xml"}));							loadque.load(true);								var fs:Screensize = new Screensize(40,40,20,20);			addChild(fs);			fs.fullscreenauto();		}						private function progressHandler(event:LoaderEvent):void {			trace("progress: " + event.target.progress);		}				private function errorHandler(event:LoaderEvent):void {			trace("error occured with " + event.target + ": " + event.text);		}					private function completeHandler(event:LoaderEvent) {			var photo = LoaderMax.getContent("photo");			var xml = LoaderMax.getContent("xml");						addChild(photo);			trace(xml);		}	}}